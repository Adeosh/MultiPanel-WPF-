<Application x:Class="MultiPanel_WPF_.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MultiPanel_WPF_"
             StartupUri="MainWindow.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionaries/ButtonDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/ComboBoxDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/LabelDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/GridViewDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
